  "dependencies": {
    "@oclif/command": "1.8.0",
    "@oclif/config": "1.17.0",
    "@oclif/plugin-help": "3.2.2",
    "chalk": "4.1.2",
    "cosmiconfig": "7.0.1",
    "detect-package-manager": "1.1.0",
    "fs-extra": "8.1.0",
    "inquirer": "8.1.1",
    "inquirer-fuzzy-path": "2.3.0",
    "pluralize": "8.0.0",
    "ts-morph": "11.0.0",
    "tslib": "1.14.1",
    "ts-node": "8.10.2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "1.26.0",
    "@types/chalk": "2.2.0",
    "@types/cosmiconfig": "6.0.0",
    "@types/fs-extra": "8.1.0",
    "@types/inquirer": "7.3.2",
    "@types/jest": "26.0.23",
    "@types/node": "10.17.60",
    "globby": "10.0.2",
    "jest": "27.0.6",
    "ts-jest": "27.0.3",
    "typescript": "4.3.5"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
