{
  "name": "elf",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "c": "git-cz",
    "start:ng": "nx serve --project ng",
    "start:react": "nx serve --project react",
    "start:docs": "npm start --prefix=docs",
    "test": "npm run affected:test",
    "test:cli": "npm test --prefix packages/cli",
    "test:all": "nx run-many --target=test --exclude=ng,react --all",
    "lint:all": "nx run-many --target=lint --all --exclude=ng,react",
    "build:all": "nx run-many --target=build --all --exclude=ng,react",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:test": "nx affected:test --base=dev --exclude=ng,react",
    "affected:lint": "nx affected:lint --base=dev --exclude=ng,react",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "update": "nx migrate latest",
    "migration": "nx migrate --run-migrations",
    "postinstall": "ngcc --properties es2015 browser module main",
    "cli": "./packages/cli/bin/run",
    "cz": "cz",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "13.0.2",
    "@angular/common": "13.0.2",
    "@angular/compiler": "13.0.2",
    "@angular/core": "13.0.2",
    "@angular/forms": "13.0.2",
    "@angular/platform-browser": "13.0.2",
    "@angular/platform-browser-dynamic": "13.0.2",
    "@angular/router": "13.0.2",
    "@ngneat/use-observable": "1.0.0",
    "core-js": "3.6.5",
    "immer": "9.0.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "5.3.0",
    "regenerator-runtime": "0.13.7",
    "rxjs": "7.3.0",
    "tslib": "2.1.0",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.0.3",
    "@angular-eslint/eslint-plugin": "12.6.1",
    "@angular-eslint/eslint-plugin-template": "12.6.1",
    "@angular-eslint/template-parser": "12.6.1",
    "@angular/compiler-cli": "13.0.2",
    "@angular/language-service": "13.0.2",
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@ngneat/until-destroy": "8.1.1",
    "@nrwl/angular": "13.2.1",
    "@nrwl/cli": "13.2.1",
    "@nrwl/eslint-plugin-nx": "13.2.1",
    "@nrwl/jest": "13.2.1",
    "@nrwl/linter": "13.2.1",
    "@nrwl/react": "13.2.1",
    "@nrwl/tao": "13.2.1",
    "@nrwl/web": "13.2.1",
    "@nrwl/workspace": "13.2.1",
    "@types/faker": "5.5.7",
    "@types/jest": "27.0.2",
    "@types/lodash": "4.14.171",
    "@types/node": "14.14.37",
    "@types/react": "17.0.30",
    "@types/react-dom": "17.0.9",
    "@types/react-router-dom": "5.3.1",
    "@typescript-eslint/eslint-plugin": "4.31.2",
    "@typescript-eslint/parser": "4.31.2",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "dotenv": "10.0.0",
    "eslint": "7.23.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.26.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "expect-type": "0.12.0",
    "faker": "5.5.3",
    "git-cz": "4.7.1",
    "husky": "6.0.0",
    "jest": "27.2.3",
    "jest-preset-angular": "11.0.0",
    "lint-staged": "11.1.2",
    "lodash": "4.17.21",
    "prettier": "2.4.1",
    "ts-jest": "27.0.5",
    "ts-node": "9.1.1",
    "typescript": "4.4.4"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{ts,json,html,tsx}": "prettier --write"
  }
