{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "generators": {
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    },
    "@nrwl/react": {
      "application": {
        "style": "scss",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "scss"
      },
      "library": {
        "style": "scss",
        "linter": "eslint"
      }
    }
  },
  "projects": {
    "cli": {
      "root": "packages/cli",
      "sourceRoot": "packages/cli/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/workspace:tsc",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/packages/cli",
            "main": "packages/cli/src/index.ts",
            "tsConfig": "packages/cli/tsconfig.lib.json",
            "assets": ["packages/cli/*.md"]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["packages/cli/**/*.ts"]
          }
        }
      }
    },
    "cli-ng": {
      "root": "packages/cli-ng",
      "sourceRoot": "packages/cli-ng/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/packages/cli-ng",
            "tsConfig": "packages/cli-ng/tsconfig.lib.json",
            "project": "packages/cli-ng/package.json",
            "entryFile": "packages/cli-ng/src/index.ts",
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [
              {
                "glob": "packages/cli-ng/*.md",
                "input": ".",
                "output": "."
              }
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["packages/cli-ng/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/packages/cli-ng"],
          "options": {
            "jestConfig": "packages/cli-ng/jest.config.js",
            "passWithNoTests": true
          }
        },
        "version": {
          "executor": "@jscutlery/semver:version",
          "options": {
            "syncVersions": false
          }
        }
      }
    },
    "devtools": {
      "root": "packages/devtools",
      "sourceRoot": "packages/devtools/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/packages/devtools",
            "tsConfig": "packages/devtools/tsconfig.lib.json",
            "project": "packages/devtools/package.json",
            "entryFile": "packages/devtools/src/index.ts",
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [
              {
                "glob": "packages/devtools/*.md",
                "input": ".",
                "output": "."
              }
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["packages/devtools/**/*.ts"]
          },
          "outputs": ["{options.outputFile}"]
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/packages/devtools"],
          "options": {
            "jestConfig": "packages/devtools/jest.config.js",
            "passWithNoTests": true
          }
        },
        "version": {
          "executor": "@jscutlery/semver:version",
          "options": {
            "syncVersions": false
          }
        }
      }
    },
    "entities": {
      "root": "packages/entities",
      "sourceRoot": "packages/entities/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/packages/entities",
            "tsConfig": "packages/entities/tsconfig.lib.json",
            "project": "packages/entities/package.json",
            "entryFile": "packages/entities/src/index.ts",
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [
              {
                "glob": "packages/entities/*.md",
                "input": ".",
                "output": "."
              }
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["packages/entities/**/*.ts"]
          },
          "outputs": ["{options.outputFile}"]
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/packages/entities"],
          "options": {
            "jestConfig": "packages/entities/jest.config.js",
            "passWithNoTests": true
          }
        },
        "version": {
          "executor": "@jscutlery/semver:version",
          "options": {
            "syncVersions": false
          }
        }
      }
    },
    "mocks": {
      "root": "packages/mocks",
      "sourceRoot": "packages/mocks/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["packages/mocks/**/*.ts"]
          },
          "outputs": ["{options.outputFile}"]
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/packages/mocks"],
          "options": {
            "jestConfig": "packages/mocks/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "ng": {
      "projectType": "application",
      "root": "apps/ng",
      "sourceRoot": "apps/ng/src",
      "prefix": "elf",
      "targets": {
        "build": {
          "executor": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/ng",
            "index": "apps/ng/src/index.html",
            "main": "apps/ng/src/main.ts",
            "polyfills": "apps/ng/src/polyfills.ts",
            "tsConfig": "apps/ng/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": ["apps/ng/src/favicon.ico", "apps/ng/src/assets"],
            "styles": ["apps/ng/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/ng/src/environments/environment.ts",
                  "with": "apps/ng/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "executor": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "ng:build:production"
            },
            "development": {
              "browserTarget": "ng:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "executor": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ng:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/ng/src/**/*.ts", "apps/ng/src/**/*.html"]
          },
          "outputs": ["{options.outputFile}"]
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/ng"],
          "options": {
            "jestConfig": "apps/ng/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "pagination": {
      "root": "packages/pagination",
      "sourceRoot": "packages/pagination/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/packages/pagination",
            "tsConfig": "packages/pagination/tsconfig.lib.json",
            "project": "packages/pagination/package.json",
            "entryFile": "packages/pagination/src/index.ts",
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [
